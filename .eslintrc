{
	"extends": [
		"airbnb-base",
		"plugin:prettier/recommended",
		"plugin:import/recommended"
	],
	"plugins": ["prettier", "import"],
	"rules": {
		"prettier/prettier": "error",
		"no-underscore-dangle": [
			"error",
			{
				"allow": ["_id"]
			}
		]
	},
	"parserOptions": {
		"ecmaVersion": 2021,
		"sourceType": "module"
	}
}
